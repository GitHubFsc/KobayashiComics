/* pages/news/index.wxss */
.container{
  position: fixed;
  top: 0;
  left: 0;
  width: 710rpx;
  height: 96.3%;
  background-color: #fafafc;
  padding: 20rpx;
}
.newsList{
  width: 710rpx;
  height: 100%;
  margin: 0rpx auto;
  background-color:  #ffffff;
}
.news{
  width: 670rpx;
  height: 140rpx;
  margin: 0 auto;
  line-height: 140rpx;
  display: flex;
  border-bottom: 1rpx solid #e5e5e5;
}
.news .news_img{
  width: 64rpx;
  height: 64rpx;
}
.news .news_img image{
  vertical-align: middle;
}
.news_title{
  font-size:30rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(51,51,51,1);
  margin: 0 430rpx 0 20rpx;
}
.news_num{
  width:36rpx;
  height:36rpx;
  border-radius: 50%;
  background:rgba(235,66,66,1);
  text-align: center;
  line-height: 36rpx;
  font-size:22rpx;
  color:rgba(255,255,255,1);
  margin-top: 50rpx;
}