/* pages/shoppingVoucher/index.wxss */
.container{
  width: 100%;
  height: 100%;
  background-color: #FAFAFC;
}
.div{
  width: 100%;
  height: 1rpx;
  background-color: #fff;
}
.tree_detail{
  margin-top: 0;
}
.nav_tabs {
  width: 100%;
  height: 70rpx;
  padding-top: 30rpx;
}
.nav_tabs_hr{
  width: 60rpx;
}
.shoppingVouchers{
  width: 718rpx;
  height: auto;
  padding: 20rpx 16rpx;
}
.shoppingVoucher{
  width: 718rpx;
  height: 208rpx;
  position: relative;
  margin: 10rpx 0;
}
.bg_img{
  width: 718rpx;
  height: 208rpx;
  position: absolute;
  top: 0;
  left: 0;
}
.sv_box{
  width: 100%;
  height: 100%;
  position: absolute;
  z-index: 2;
  display: flex;
}
.priceR{ 
  font-size:40rpx;
  font-family:PingFangSC-Medium,PingFang SC;
  font-weight:500;
  color:rgba(203,1,1,1);
  position: relative;
  line-height: 208rpx;
  margin-left: 53rpx;
}
.priceG{
  font-size:40rpx;
  font-family:PingFangSC-Medium,PingFang SC;
  font-weight:500;
  color:rgba(153,153,153,1);
  position: relative;
  line-height: 208rpx;
  margin-left: 53rpx;
}
.priceR::after,.priceG::after{ 
  content: "";
  display: inline-block;
  width:2rpx;
  height:48rpx;
  background:rgba(216,216,216,1);
  position: absolute;
  right: -23rpx;
  top: 78rpx;
}
.sv_box_txt{
  margin-left: 78rpx;
  margin-top: 48rpx;
  line-height: 56rpx;
}
.sv_title{
  font-size:28rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(51,51,51,1);
}
.sv_txt{
  font-size:24rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(153,153,153,1);
}
.svb_btnR{
  width:120rpx;
  height:52rpx;
  background:rgba(255,255,255,1);
  border-radius:28px;
  border:1px solid rgba(203,1,1,1);
  text-align: center;
  line-height: 52rpx;
  font-size:24rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(203,1,1,1);
  margin-top: 76rpx;
  margin-left: 100rpx;
}
.svb_btnG{
  width:120rpx;
  height:52rpx;
  background:rgba(255,255,255,1);
  border-radius:28rpx;
  border:1px solid rgba(153,153,153,1);
  text-align: center;
  line-height: 52rpx;
  font-size:24rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(153,153,153,1);
  margin-top: 76rpx;
  margin-left: 100rpx;
}
