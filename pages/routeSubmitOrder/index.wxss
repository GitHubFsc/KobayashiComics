/* pages/routeSubmitOrder/index.wxss */
.container {
  width: 100%;
  height: 100%;
}

.commoditys {
  width: 710rpx;
  height: auto;
  padding: 20rpx;
  background-color: #fafafc;
}

.commoditys .more {
  width: 14rpx;
  height: 24rpx;
}

.commodity_top {
  width: 670rpx;
  height: auto;
  padding: 20rpx;
  background-color: #ffffff;
  display: flex;
}

.ct_img {
  width: 240rpx;
  height: 156rpx;
  margin-right: 20rpx;
}

.ct_right {
  width: auto;
  height: auto;
  line-height: 40rpx;
}

.ct_title {
  font-size: 30rpx;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
}

.ct_txt {
  font-size: 26rpx;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
}

.ct_price {
  width: 100%;
  display: flex;
  justify-content: space-between;
  font-size: 28rpx;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
  margin-top: 36rpx;
}

.price {
  color: rgba(203, 1, 1, 1);
}
.commodity_information{
  margin: 20rpx 0;
}
.commodity_discount{
  margin-bottom: 100rpx;
}
.commodity_discount,.commodity_information{
  width: 670rpx;
  height: auto;
  padding: 20rpx;
  background-color: #ffffff;
  font-size:26rpx;
  font-weight:400;
  color:rgba(51,51,51,1);
}


.commoditys .ci_cell{
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  display: flex;
  border-bottom: 1rpx solid #e5e5e5;
}
.commoditys .ci_cell view:nth-child(1){
  width: 210rpx;
}
.commoditys .ci_cell view:nth-child(2){
  width: 420rpx;
  height: 90rpx;
}
.commoditys .ci_cell input{
  width: 444rpx;
  height: 90rpx;
  color: #999999;
}
.commoditys .cd_cell .switch {
  transform:scale(0.7,0.7);
  margin-left: 120rpx;
}
.commodity_discount .cd_cell{
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  display: flex;
  justify-content: space-between;
  border-bottom: 1rpx solid #e5e5e5;
}
.commodity_discount .cd_cell .cdc_txt{
  width: 562rpx;
  height: auto;
  text-align: right;
}
.topay{
  width: 100%;
  height: 98rpx;
  position: fixed;
  left: 0 ;
  bottom: 0 ;
  background-color:rgba(255,255,255,1);
  display: flex;
  border-top: 1rpx solid #e5e5e5;
}
.topay view:nth-child(1){
  width: 550rpx;
  font-size:26rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(153,153,153,1);
  line-height: 122rpx;
  text-indent: 20rpx;
}
.topay_price{
  font-size: 32rpx;
  color: rgba(203, 1, 1, 1);
}
.topay view:nth-child(2){
  width:200rpx;
  height:98rpx;
  text-align: center;
  line-height: 98rpx;
  background:rgba(203,1,1,1);
  font-size:32rpx;
  font-weight:500;
  color:rgba(255,255,255,1);
}
.peopleBox{

}


.calendar_box,.peopleBox{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background-color: rgba(0, 0, 0, 0.5);
  z-index: 100;
}

.calendar_box .calendars ,.peopleBox .people_box{
  width: 710rpx;
  height: auto;
  padding: 20rpx;
  position: absolute;
  bottom: 0;
  left: 0;
  background-color: rgb(255, 255, 255);
}

.calendar_box .calendars .calendar_top {
  width: 100%;
  height: auto;
  padding:  16rpx 0;
  display: flex;
  justify-content: space-between;
  font-size:28rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(51,51,51,1);
}

.calendar_box .calendars .calendar_top .close {
  width: 36rpx;
  height: 36rpx;
}

.calendar_box .calendars .calendar_txt {
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
  text-align: center;
}
.calendar_box .calendars .calendar_nav{
  width: 100%;
  height: auto;
  padding: 16rpx 0;
  display: flex;
  justify-content: space-around;
}
.calendar_box .calendars .calendar {
  width: 100%;
  height: auto;
}
.calendar-title{
  text-align: center;
}
.calendar swiper {
  width: 100%;
  height: 600rpx;
}
.pb_top{
  width:100%;
  font-size: 26rpx;
  color: #5e95de;
  display: flex;
  justify-content: space-between;
}
.pb_title{
  font-size: 30rpx;
  color: #333;
}
.pb_users{
  width: 100%;
  font-size: 28rpx;
  color: #333;
}
.pb_user{
  width: 100%;
  height: 120rpx;
  line-height: 120rpx;
  display: flex;
  justify-content: space-between;
  border-bottom: 1rpx solid #e5e5e5;
}
.pb_user text:nth-child(1){
  margin-right: 40rpx;
}
.pbu_img{
  width: 36rpx;
  height: 36rpx;
  margin-top: 10rpx;
}
.addResident{
  width: 100%;
  height: 120rpx;
  line-height: 120rpx;
  text-align: center;
  color: #d40001;
}